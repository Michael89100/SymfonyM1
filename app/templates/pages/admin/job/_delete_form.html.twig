<form method="post" action="{{ path('job.delete', {'id': job.id}) }}" onsubmit="return confirm('Êtes-vous sûre de vouloir supprimer ce métier');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ job.id) }}">
    <button class="btn btn-warning mt-4" class="btn">Supprimer</button>
</form>
