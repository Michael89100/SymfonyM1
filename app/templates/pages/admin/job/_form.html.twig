{{ form_start(form) }}
    <div class="form-error">
        {{ form_errors(form) }}
    </div>
    <div class="row">
        <div class="form-group">
            {{ form_label(form.name) }}
            {{ form_widget(form.name) }}
            <div class="form-error">
                {{ form_errors(form.name) }}
            </div>
        </div>
        <div class="form-group">
            {{ form_row(form.activities) }}
        </div>
        <div class="form-group">
            {{ form_row(form.skills) }}
        </div>
        <div class="form-group">
            {{ form_row(form.submit) }}
        </div>
    </div>
{{ form_end(form) }}
